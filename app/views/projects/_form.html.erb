<%= render 'layouts/errors', :object => @project %>

<%= form_for [@skill, @project] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :link %>
  <%= f.text_field :link, placeholder: "www.github.com/username/project" %>
  <p><%= f.submit %></p>
<% end %>
